{
	"name": "genz_market",
	"version": "1.0.0",
	"description": "A freelance marketplace for Kenyan Youth",
	"main": "index.js",
	"scripts": {
		"start": "webpack serve --config webpack.config.dev.js --port 3000",
		"test": "jest --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/whirlLoop/genz_market.git"
	},
	"keywords": [
		"freelance",
		"genz",
		"youth",
		"Kenya"
	],
	"author": "xcixor",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/whirlLoop/genz_market/issues"
	},
	"homepage": "https://github.com/whirlLoop/genz_market#readme",
	"dependencies": {
		"@emotion/react": "^11.10.0",
		"@emotion/styled": "^11.10.0",
		"@fortawesome/fontawesome-svg-core": "^6.2.0",
		"@fortawesome/free-solid-svg-icons": "^6.2.0",
		"@fortawesome/react-fontawesome": "^0.2.0",
		"@mui/material": "^5.10.1",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-country-region-selector": "^3.6.1",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.3.0"
	},
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@babel/eslint-parser": "^7.18.9",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.5",
		"babel-preset-react-app": "^10.0.1",
		"css-loader": "^6.7.1",
		"cssnano": "^5.1.13",
		"eslint": "^8.22.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-react": "^7.30.1",
		"eslint-webpack-plugin": "^3.2.0",
		"html-webpack-plugin": "^5.5.0",
		"jest": "^29.0.3",
		"jest-environment-jsdom": "^29.0.3",
		"mini-css-extract-plugin": "^2.6.1",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.16",
		"postcss-loader": "^7.0.1",
		"react-test-renderer": "^18.2.0",
		"style-loader": "^3.3.1",
		"webpack": "^5.74.0",
		"webpack-bundle-analyzer": "^4.5.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.10.0"
	},
	"jest": {
		"setupFilesAfterEnv": [
			"./tools/setupTest.js"
		],
		"testEnvironment": "jsdom",
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/fileMock.js",
			"\\.(css|less)$": "<rootDir>/tools/styleMock.js"
		}
	},
	"babel": {
		"presets": [
			"babel-preset-react-app"
		]
	},
	"eslintConfig": {
		"extends": [
			"eslint:recommended",
			"plugin:react/recommended",
			"plugin:import/errors",
			"plugin:import/warnings"
		],
		"parser": "babel-eslint",
		"parserOptions": {
			"ecmaVersion": 2018,
			"sourceType": "module",
			"ecmaFeatures": {
				"jsx": true
			}
		},
		"env": {
			"browser": true,
			"node": true,
			"es6": true,
			"jest": true
		},
		"rules": {
			"no-debugger": "off",
			"no-console": "off",
			"no-unused-vars": "warn",
			"react/prop-types": "warn",
			"no-mixed-spaces-and-tabs": 0
		},
		"settings": {
			"react": {
				"version": "detect"
			}
		},
		"root": true
	}
}
